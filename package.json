{
  "name": "cbr-api",
  "version": "0.0.2",
  "description": "CBR API Wrapper",
  "module": "./dist/index.js",
  "main": "./cjs/index.js",
  "type": "module",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "build:cjs": "rm -rf cjs && tsc -p tsconfig.build.cjs.json && echo '{\"type\":\"commonjs\"}' > cjs/package.json",
    "build:esm": "rm -rf dist && tsc -p tsconfig.build.json",
    "build": "npm run build:esm && npm run build:cjs"
  },
  "author": "Nik Shevchenko <shevchenko.nik@icloud.com>",
  "license": "MIT",
  "keywords": [
    "central bank",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shevchenkonik/cbr-api"
  },
  "devDependencies": {
    "@types/jest": "^29.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "fast-xml-parser": "^4.3.2"
  }
}
